    "karma-jspm": "2.2.0",
    "object.assign": "^4.0.4",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.2.2",
    "vinyl-paths": "^2.1.0",
    "yargs": "^4.8.1"
  },
  "jspm": {
    "registry": "npm",
    "jspmPackage": true,
    "main": "index",
    "format": "amd",
    "directories": {
      "dist": "dist/amd"
    },
    "peerDependencies": {},
    "devDependencies": {
      "aurelia-polyfills": "^1.0.0-beta.1.1.0"
    }
  }
